{
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": ".",
    "outDir": "build",
    "module": "ESNext",
    "target": "es2016",
    "lib": [
      "DOM",
      "ESNext"
    ],
    "strict": true,
    "allowJs": true,
    "esModuleInterop": true,
    "incremental": false,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "types": [
      "vitest",
      "vite/client",
      "vue/ref-macros",
      "vite-plugin-pages/client",
      "vite-plugin-vue-component-preview/client",
      "kapix-vite-plugin-vue-layouts/client",
      "vite-plugin-pwa/client",
      "unplugin-vue-define-options/macros-global"
    ],
    "paths": {
      "~/*": [
        "src/*"
      ]
    }
  },
  "exclude": [
    "dist",
    "node_modules",
    "cypress"
  ]
}