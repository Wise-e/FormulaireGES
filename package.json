{
  "name": "vuejs-code-generation",
  "private": true,
  "type": "module",
  "funding": "https://www.kapix.fr",
  "bugs": "https://discord.com/invite/PzRymg6VV9",
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "scripts": {
    "start": "pnpm install & pnpm lint --fix & pnpm dev",
    "update-kapix": "pnpm install & pnpm upgrade kapix-components-vue3@latest",
    "build": "cross-env NODE_ENV=production vite-ssg build",
    "build:no-minify": "cross-env NODE_ENV=no-minify vite-ssg build",
    "build-no-ssg": "cross-env NODE_ENV=production vite build",
    "build-no-ssg:no-minify": "cross-env NODE_ENV=no-minify vite build",
    "dev": "vite --port 3333 --open",
    "netlify-create-site": "node netlify-site",
    "netlify-publish": "cross-env NODE_ENV=production cross-env-shell netlify deploy --dir dist --site $NETLIFY_SITE_NAME --auth $NETLIFY_AUTH_TOKEN --prod",
    "netlify-published": "node netlify-published",
    "lint": "eslint \"src/**/*.{vue,ts,js}\"",
    "lint:fix": "pnpm lint --fix",
    "test": "cypress open",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "typecheck": "vue-tsc --noEmit"
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "popper.js": "1.16.1",
      "sourcemap-codec": "1.4.8",
      "statsd-client": "0.4.7",
      "querystring": "0.2.0",
      "@npmcli/move-file": "2.0.1",
      "source-map-resolve": "0.5.3",
      "resolve-url": "0.2.1",
      "source-map-url": "0.4.1",
      "urix": "0.1.0",
      "rollup-plugin-terser": "7.0.2"
    }
  },
